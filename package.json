{
  "name": "fe3h-team-builder",
  "version": "0.0.1",
  "description": "A tool made for organize and optimize the team in fire emblem three houses",
  "main": "index.html",
  "scripts": {
    "build": "sass resources/sass/:resources/css/ --no-source-map && elm make src/Main.elm --output=bundle.js",
    "watch": "sass resources/sass/:resources/css/ --no-source-map && elm-live src/Main.elm --open -- --output=bundle.js",
    "test": "elm-test",
    "try": "elm-repl"
  },
  "keywords": [
    "fe3h",
    "team",
    "builder",
    "fire",
    "emblem",
    "fireemblem",
    "three",
    "houses",
    "feth",
    "class",
    "skill",
    "perk",
    "optimize",
    "optimization",
    "build",
    "organize",
    "elm",
    "elm-live",
    "elm-test",
    "elm-format"
  ],
  "author": "mcdouglas",
  "license": "MIT",
  "devDependencies": {
    "elm": "^0.19.1-6",
    "elm-live": "^4.0.2",
    "sass": "^1.97.2"
  }
}
